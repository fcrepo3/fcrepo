<?xml version="1.0"?>
<betwixt-config>
	<class name="fedora.server.security.servletfilters.xmluserfile.FedoraUsers">
		<element name="users">
			<element name="role" property="roles" />
			<element name="user" property="users" />
			<addDefaults />
		</element>
	</class>
	
	<class name="fedora.server.security.servletfilters.xmluserfile.Role">
		<element name="role">
			<attribute name="rolename" property="rolename" />
			<addDefaults/>
		</element>
	</class>
	
	<class name="fedora.server.security.servletfilters.xmluserfile.User">
		<element name="user">
			<attribute name="name" property="name" />
			<attribute name="password" property="password" />
			<attribute name="roles" property="roles" />
			<element name="attribute" property="attributes" />
			<addDefaults/>
		</element>
	</class>
	
	<class name="fedora.server.security.servletfilters.xmluserfile.Attribute">
		<element name="attribute">
			<attribute name="name" property="name" />
			<element name="value" property="values" />
			<addDefaults/>
		</element>
	</class>
</betwixt-config>